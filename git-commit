+    <script>
+        // World map data - simplified version
+        const worldMapData = {
+            mapImage: {
+                src: "images/map.jpg",
+                height: 400,
+                width: 900,
+                text: "World Map",
+                alt: "D.E. Technics Global Presence Map",
+                class: "world-map-image"
+            }
+        };
+
+        // Function to initialize the map
+        function initMap() {
+            const svg = document.querySelector('.world-map');
+            
+            // Add the background map image
+            const mapImage = document.createElementNS('http://www.w3.org/2000/svg', 'image');
+            mapImage.setAttribute('href', worldMapData.mapImage.src);
+            mapImage.setAttribute('x', '50');
+            mapImage.setAttribute('y', '50');
+            mapImage.setAttribute('width', '900');
+            mapImage.setAttribute('height', '400');
+            svg.appendChild(mapImage);
+        }
+
+        // Initialize the map when the page loads
+        window.addEventListener('load', initMap);
+
+        // Flag item click handler
+        document.querySelectorAll('.flag-item').forEach(item => {
+            item.addEventListener('click', function() {
+                const countryName = this.getAttribute('data-country');
+                // Highlight the country on the map (visual feedback only)
+                // Since we've removed pins, we'll just scroll to the map
+                const mapContainer = document.querySelector('.world-map-container');
+                mapContainer.scrollIntoView({ behavior: 'smooth', block: 'center' });
+            });
+        });
+
+        // Mobile menu toggle
+        document.querySelector('.mobile-menu-toggle').addEventListener('click', function() {
+            const nav = document.querySelector('.main-nav');
+            nav.style.display = nav.style.display === 'block' ? 'none' : 'block';
+        });
+    </script>
+</body>
+</html>
